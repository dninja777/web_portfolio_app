<h1>Game Art</h1>

<div class="transitions-enabled" id="posts">
    <% @posts.each do |post| %>
        <div class="box panel panel-default">
            
        <%= link_to (image_tag post.image.url), post %>
        </div>
        <div class="panel-body">
            <p> <%= post.title %> </p>
            
        </div>
         
        
   <% end %>
    
</div>

